{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "../auth-service"],
  "files.watcherExclude": {
  "**/node_modules/**": true,
  "**/.git/**": true
},
"typescript.tsserver.maxTsServerMemory": 3072
} 