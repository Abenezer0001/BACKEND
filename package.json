{
  "name": "inseat-backend",
  "version": "1.0.0",
  "description": "Restaurant Management System Backend Services",
  "scripts": {
    "build": "tsc",
    "start": "ts-node -r tsconfig-paths/register ./src/app.ts",
    "dev": "nodemon",
    "dev:watch": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/app.ts",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "seed": "npx ts-node -r tsconfig-paths/register services/restaurant-service/src/seeds/seed.ts",
    "seed:menus": "npx ts-node -r tsconfig-paths/register services/restaurant-service/src/seeds/seed-menus.ts",
    "seed:categories": "npx ts-node -r tsconfig-paths/register services/restaurant-service/src/seeds/seed-categories.ts",
    "seed:menu-data": "npx ts-node -r tsconfig-paths/register src/seedMenu.ts",
    "seed:menu-js": "node src/seedMenu.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.787.0",
    "@aws-sdk/lib-storage": "^3.844.0",
    "@types/multer": "^1.4.12",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "amqplib": "^0.10.3",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.1",
    "express-winston": "^4.2.0",
    "formidable": "^3.5.2",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "mongoose": "^7.8.6",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "qrcode": "^1.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "socket.io": "^4.7.2",
    "stripe": "^14.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsconfig-paths": "^3.14.2",
    "typescript": "^5.2.2",
    "uuid": "^11.1.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@prisma/client": "^6.5.0",
    "@types/amqplib": "^0.10.6",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/mongoose": "^5.11.96",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.10",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/qrcode": "^1.5.5",
    "@types/socket.io": "^3.0.1",
    "@types/supertest": "^6.0.2",
    "@types/ws": "^8.18.0",
    "cursor-tools": "latest",
    "eslint": "^8.30.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0"
  }
}
